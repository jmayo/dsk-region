@font-face
	font-family "iconos"
	src url("../fonts/icomoon.eot?d39ynr")
	src url("../fonts/icomoon.eot?#iefixd39ynr") format("embedded-opentype"),
		url("../fonts/icomoon.woff?d39ynr") format("woff"),
		url("../fonts/icomoon.ttf?d39ynr") format("truetype"),
		url("../fonts/icomoon.svg?d39ynr#icomoon") format("svg")
	font-weight normal
	font-style normal

nav
	background gris_oscuro
	position fixed
	top 0
	width 100%
	z-index 9
	.menu
		overflow-x auto
		margin 0
		padding 0
		li
			display inline-block
			padding 0.5em
			a
				color gris_claro
				text-decoration none
			a.personal
				&:before
					border-style solid
					border-color rosa
					border-radius 10px
					border-width 2px
					font-family ico_font_family
					font-size 40px
					content ico_personal
				&:hover:before
					color rosa
					border-color gris_claro
					box-shadow 10px 10px 25px lightblue
			a.empresas
				&:before
					border-style solid
					border-color rosa
					border-radius 10px
					border-width 2px
					font-family ico_font_family
					font-size 40px
					content ico_empresas
				&:hover:before
					color rosa
					border-color gris_claro
					box-shadow 10px 10px 25px lightblue
			a.catalogos
				&:before
					border-style solid
					border-color rosa
					border-radius 10px
					border-width 2px
					font-family ico_font_family
					font-size 40px
					content ico_catalogos
				&:hover:before
					color rosa
					border-color gris_claro
					box-shadow 10px 10px 25px lightblue
			a.cerrar_sesion
				color rosa
				&:before
					font-family ico_font_family
					font-size 25px
					content ico_cerrar_sesion
				&:hover:before
					color gris_claro

.caja_acciones
	background gris_claro
	margin-top -1em
	padding 0
	position fixed
	text-align center
	width 100%
	z-index 8
	.menu_acciones
		overflow-x auto
		margin 4px
		padding 0
		li
			display inline-block
			padding-left 0.5em
			a
				color gris_medio
				text-decoration none
			a.nuevo
				&:before
					font-family ico_font_family
					font-size 30px
					content ico_nuevo
			a.eliminar
				&:before
					font-family ico_font_family
					font-size 30px
					content ico_eliminar
			a.guardar
				&:before
					font-family ico_font_family
					font-size 30px
					content ico_guardar
			input.buscar
				border 2px solid gris_medio
				border-radius 6px
				color black
				height 1.5em
				width 18em
				&:focus
					border 2px solid rosa

body
	background -moz-linear-gradient(bottom,  rgba(0,0,0,0.65) 0%, rgba(0,0,0,0) 100%) /* FF3.6+ */
	background -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0.65)), color-stop(100%,rgba(0,0,0,0))) /* Chrome,Safari4+ */
	background -webkit-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%) /* Chrome10+,Safari5.1+ */
	background -o-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%) /* Opera 11.10+ */
	background -ms-linear-gradient(top,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%) /* IE10+ */
	background linear-gradient(to bottom,  rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%) /* W3C */
	font-family arial-regular
	font-size 14px
	margin-top 5em

.contenido
	font-family arial-regular
	.div_empresa_servicio
		display inline-block
		width 25%
		vertical-align top
		margin-right 1%
		margin-left 1%
		margin-top 4em
	.div_empresa
		background gris_oscuro
		border-radius 0 0 0.5em 0.5em
		box-shadow 2px 2px 10px gris_claro
		color white
		font-size 16px
		text-align left
		position relative
		margin-bottom 0.2em
		padding 2px 50px 2px 10px
	.agregar_servicio
		position absolute
		margin-left 19.6em
		margin-top -2.8em
		a
			color rosa
			text-decoration none
			&:before
				padding 2px
				border-style solid
				border-color rosa
				border-radius 100px
				border-width 2px				
				font-family ico_font_family
				font-size 20px
				content ico_agregar
	a.lnk_servicio
		text-decoration none
		display inline-block
		vertical-align middle
		-webkit-transform translateZ(0)
		transform translateZ(0)
		box-shadow 0 0 2px rgba(0, 0, 0, 0)
		-webkit-backface-visibility hidden
		backface-visibility hidden
		-moz-osx-font-smoothing grayscale
		position relative
		overflow hidden
		width 100%
		&:before
			content ""
			position absolute
			z-index -1
			left 50%
			right 50%
			bottom 0
			background rosa
			height 4px
			-webkit-transition-property left, right
			transition-property left, right
			-webkit-transition-duration 0.8s
			transition-duration 0.8s
			-webkit-transition-timing-function ease-out
			transition-timing-function ease-out
		&:hover:before, &:focus:before, &:active:before
			left 0
			right 0
	.div_servicio
		background gris_medio
		border-radius 0 0 0.5em 0.5em
		box-shadow 2px 2px 10px gris_claro
		color white
		font-size 12px
		text-align left
		margin-bottom 0.5em
		padding 8px 30px
//#bloque_empresa
#bloque_contacto
	display none
.bloque_mapa
	display inline-block
	width 30%
	vertical-align top
	margin-right 1%
	margin-left 1%
	margin-top 4em
	margin-bottom 4em
	.titulo_mapa
		background verde
		border-radius 5px 5px 0 0
		color white
		font-family arial-regular
		font-size 16px
		text-align center
	.caja_mapa
		background lightblue
		border-radius 0 0 5px 5px
		height 24em
.bloque
	display inline-block
	width 30%
	vertical-align top
	margin-right 1%
	margin-left 1%
	margin-top 4em
	margin-bottom 4em
	#titulo_bloque_sucursal
		background rosa
	.titulo_bloque
		background verde
		border-radius 5px 5px 0 0
		color white
		font-family arial-regular
		font-size 16px
		text-align center
	.caja_bloque
		border-radius 0 0 5px 5px
		background gris_medio
		.campos_bloque
			.ul_bloque
				width 800px
				list-style-type none
				margin 0
				padding 0.1em
				.li_bloque
					color gris_claro
					font-family arial-regular
					font-size 14px
					padding 0.2em
					.etiquetas_bloque
						width 135px
						margin-top 2px
						display inline-block
						float left
						padding 2px
					.inputs_bloque
						color black
						height 16px
						width 220px
						padding 2px 8px
						border 1px solid lightblue
						box-shadow 0px 0px 1px verde, 0 10px 15px gris_claro inset
						border-radius 1px
						&:focus
							border 1px solid white
						&:valid
							border 1px solid lightgreen
						&:valid ~ .viñeta::before
							content "\2713"
							color lightgreen
						&:invalid
							border 1px solid rojo_vineta
						&:invalid ~ .viñeta::before
							content "\2613"
							color rojo_vineta
					.viñeta
						display inline-block
					.textarea_bloque
						color black
						height 80px
						width 220px
						resize vertical
						padding 2px 8px
						border 1px solid lightblue
						box-shadow 0px 0px 1px verde, 0 10px 15px gris_claro inset
						border-radius 1px
						&:focus
							border 1px solid white
						&:valid
							border 1px solid lightgreen
						&:valid ~ .viñeta::before
							content "\2713"
							color lightgreen
						&:invalid
							border 1px solid rojo_vineta
						&:invalid ~ .viñeta::before
							content "\2613"
							color rojo_vineta
					.viñeta
						display inline-block
					.select_bloque
						color black
						height 22px
						width 238px
						padding 2px 8px
						border 1px solid lightblue
						box-shadow 0px 0px 1px verde, 0 10px 15px gris_claro inset
						border-radius 1px
						&:focus
							border 1px solid white
						&:valid
							border 1px solid lightgreen
						&:valid ~ .viñeta::before
							content "\2713"
							color lightgreen
						&:invalid
							border 1px solid rojo_vineta
						&:invalid ~ .viñeta::before
							content "\2613"
							color rojo_vineta
					.viñeta
						display inline-block
					.radio_bloque:not(old)
						width 2em
						margin 0
						padding 0
						font-size 1em
						opacity 0
						&:not(old) + label
							display inline-block
							margin-left -2em
							line-height 1.5em
						&:not(old) + label > span
							display inline-block
							width 0.875em
							height 0.875em
							margin 0.25em 0.5em 0.25em 0.25em
							border 0.0625em solid gris_claro_rgb_1
							border-radius 0.25em
							background gris_claro_rgb_2
							background-image -moz-linear-gradient(gris_claro_rgb_3,gris_claro_rgb_2)
							background-image -ms-linear-gradient(gris_claro_rgb_3,gris_claro_rgb_2)
							background-image -o-linear-gradient(gris_claro_rgb_3,gris_claro_rgb_2)
							background-image -webkit-linear-gradient(gris_claro_rgb_3,gris_claro_rgb_2)
							background-image linear-gradient(gris_claro_rgb_3,gris_claro_rgb_2)
							vertical-align bottom
						&:not(old):checked + label > span
							background-image -moz-linear-gradient(gris_claro_rgb_2,gris_claro_rgb_3)
							background-image -ms-linear-gradient(gris_claro_rgb_2,gris_claro_rgb_3)
							background-image -o-linear-gradient(gris_claro_rgb_2,gris_claro_rgb_3)
							background-image -webkit-linear-gradient(gris_claro_rgb_2,gris_claro_rgb_3)
							background-image linear-gradient(gris_claro_rgb_2,gris_claro_rgb_3)
						&:not(old):checked +  label > span > span
							display block
							width 0.5em
							height 0.5em
							margin 0.125em
							border 0.0625em solid verde_rgb
							border-radius 0.125em
							background verde_rgb
							background-image -moz-linear-gradient(verde_rgb,verde_rgb)
							background-image -ms-linear-gradient(verde_rgb,verde_rgb)
							background-image -o-linear-gradient(verde_rgb,verde_rgb)
							background-image -webkit-linear-gradient(verde_rgb,verde_rgb)
							background-image linear-gradient(verde_rgb,verde_rgb)
	.caja_foto
		border 0
		background gris_medio
		.foto
			margin 0
			img
				width 180px
				height 180px
				border-radius 20px
		.foto p
			margin 0
			text-align center

 .divResultados
		position absolute
		margin-left auto
		margin-right auto
		width: 320px
		left: 45%
		margin-top -10px    
		border solid 1px #dedede
		border-top 0px
		border-radius 6px
		box-shadow 0px 0px 5px #999
		border-width 1px 1px 1px
		border-style solid
		border-color gris_claro #DEDEDE #DEDEDE
		background-color white
		.resultado_ind
			padding:9px
			border-top solid 1px #dedede
			font-size 12px
			height 40px
			margin 1px 1px 1px 1px
			overflow hidden
			img
				float left
				height 40px
				width 40px
				margin-right 6px
		.resultado_ind:hover
			background gris_medio
			color #FFFFFF
			cursor pointer

footer
	background gris_oscuro
	bottom 0
	position fixed
	width 100%
	.redes_sociales
		overflow-x auto
		margin 0
		padding 0
		li
			display inline-block
			float right
			padding 0.5em
			a
				color gris_claro
				font-size 20px
				text-decoration none
			a.e-mail
				&:before
					font-family ico_font_family
					content ico_e-mail
			a.facebook
				&:before
					font-family ico_font_family
					content ico_facebook
			a.twitter
				&:before
					font-family ico_font_family
					content ico_twitter

.message
	box-shadow inset 0 -1px 0 rgba(255,255,255,.4)
	width 100%
	border 1px solid
	color #fff
	padding 15px
	position fixed
	_position absolute
	text-shadow 0 1px 0 rgba(0,0,0,.5)
	height 35px

.info
	background-color #4ea5cd
	border-color #3b8eb5

.error
	background-color #de4343
	border-color #c43d3d

.warning
	background-color #eaaf51
	border-color #d99a36

.success
	background-color #61b832
	border-color #55a12c

.message h3
	margin 0 0 5px 0
	text-align center

.message p
	margin 0
	text-align center

@media screen and (max-width: 768px)
	.contenido
		.div_empresa_servicio
			margin-top 5em
			width 100%
		.agregar_servicio
			margin-left 17.6em
			margin-top -2.8em
		.bloque_mapa
		.bloque
			margin-top 1em
			width 100%
			.titulo_mapa
			.titulo_bloque
				font-size 14px
			.caja_mapa
			.caja_bloque
				width 100%
			br
				display none
			.campos_bloque
				display flex
			.caja_mapa
				height 14.5em
	.caja_acciones
		.menu_acciones
		li
			.buscar
				height 15px
				width 250px