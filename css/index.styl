// colores
gris_claro=#dddddc
gris_oscuro=#30302f
gris_medio=#696868
rosa=#df7250
verde=#1cb28c
verde_rgb=rgb(28,178,140)
gris_claro_rgb_1=rgb(192,192,192)
gris_claro_rgb_2=rgb(224,224,224)
gris_claro_rgb_3=rgb(240,240,240)
gris_claro_rgba_1=rgba(0, 0, 0, 0.5)
rojo_vineta=#ff7070
ico_font_family="iconos"
ico_personal="\e607"
ico_empresas="\e606"
ico_catalogos="\e600"
ico_cerrar_sesion="\e609"
ico_e-mail="\e601"
ico_facebook="\e605"
ico_twitter="\e60d"
ico_nuevo="\e60a"
ico_eliminar="\e604"
ico_guardar="\e608"
ico_flecha="\e602"
ico_agregar="\e60b"

